{
    "version": 2,
    "functions": {
        "app/*.py": {
            "memory": 1024,
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app/main.py"
        }
    ],
    "builds": [
        {
            "src": "app/*.py",
            "use": "@vercel/python"
        }
    ]
}